import pandas as pd

# Äá»c dá»¯ liá»‡u (giáº£ sá»­ cÃ³ cá»™t 'City' vÃ  'Temperature')
df = pd.read_csv(r"E:\WORK\STUDY\Python\DataProcessing\temperature.csv")
print(df.head())

# Lá»c dá»¯ liá»‡u
hanoi_data = df[df['City'] == 'Hanoi']

# Trung bÃ¬nh nhiá»‡t Ä‘á»™
avg_temp = df['Temperature'].mean()
print("ğŸŒ¡ Nhiá»‡t Ä‘á»™ trung bÃ¬nh:", avg_temp)

# Group by
grouped = df.groupby('City')['Temperature'].mean()
print(grouped)
